    <title>Bing Maps Example</title>
    <script type="text/javascript" src="https://www.bing.com/api/maps/mapcontrol?key=Ar-x_oaZ5J-md-Wra2_LJEG3NJt2uG0-HyqDMad50QptHo52WOTGIJYrnl83_nZw"></script>
</head>
<body>
    <div id="map" style="width: 600px; height: 400px;"></div>
    <script>
        function loadMap() {
            var map = new Microsoft.Maps.Map('#map', {
                center: new Microsoft.Maps.Location(47.6062, -122.3321), // Example coordinates (Seattle, WA)
                zoom: 10
            });
        }
        Microsoft.Maps.loadModule('Microsoft.Maps.Map', { callback: loadMap });
    </script>
</body>
